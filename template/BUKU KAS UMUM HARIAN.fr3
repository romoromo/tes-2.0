<TfrxReport Version="4.5" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41291,5906202431" ReportOptions.Description.Text="" ReportOptions.LastChange="41711,6044191088" ScriptLanguage="PascalScript" ScriptText.Text="function isikiri( S:STRING; kap:integer): string;&#13;&#10;&#13;&#10;var&#13;&#10;&#13;&#10;  I,x: Integer;&#13;&#10;&#13;&#10;  str:string;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;while Length(s)&#62;kap do&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10; kap:=kap+1;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;if Length(s)&#60;=kap then&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;I:= kap-Length(s);&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;str:=s;&#13;&#10;&#13;&#10;for x := 1 to i do&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;insert('0',str,1);&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10; result:=str;&#13;&#10;&#13;&#10;end;  &#13;&#10;&#13;&#10;function terbilang(d:string):string;&#13;&#10;&#13;&#10;var&#13;&#10;&#13;&#10;    divX,modX,grop : string;&#13;&#10;&#13;&#10;function trimall(STR:string):string;&#13;&#10;&#13;&#10;  var&#13;&#10;&#13;&#10;  i : integer;&#13;&#10;&#13;&#10;  str2 : string;                                  &#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;  i := 0;&#13;&#10;&#13;&#10;  for i := 1 to length(str) do&#13;&#10;&#13;&#10;  begin&#13;&#10;&#13;&#10;  if (str[i] in ['0'..'9']) then str2 := str2 + str[i];                                                                                                            &#13;&#10;&#13;&#10;  end;&#13;&#10;&#13;&#10;  RESULT := str2;                                  &#13;&#10;&#13;&#10;end;          &#13;&#10;&#13;&#10;  function bil(x:integer):string;&#13;&#10;&#13;&#10;  begin&#13;&#10;&#13;&#10;    case x of&#13;&#10;&#13;&#10;      0 : result := '';&#13;&#10;&#13;&#10;      1 : result := 'Satu';&#13;&#10;&#13;&#10;      2 : result := 'Dua';              &#13;&#10;&#13;&#10;      3 : result := 'Tiga';              &#13;&#10;&#13;&#10;      4 : result := 'Empat';                &#13;&#10;&#13;&#10;      5 : result := 'Lima';              &#13;&#10;&#13;&#10;      6 : result := 'Enam';              &#13;&#10;&#13;&#10;      7 : result := 'Tujuh';                &#13;&#10;&#13;&#10;      8 : result := 'Delapan';                    &#13;&#10;&#13;&#10;      9 : result := 'Sembilan';                      &#13;&#10;&#13;&#10;      10 : result :='Sepuluh';                    &#13;&#10;&#13;&#10;      11 : result :='Sebelas';                              &#13;&#10;&#13;&#10;      end;                &#13;&#10;&#13;&#10;  end;      &#13;&#10;&#13;&#10;  function terbilang_int(x:integer):string;&#13;&#10;&#13;&#10;    begin &#13;&#10;&#13;&#10;    if (x &#60; 12) then Result := ' ' + bil(x) else if (x &#60; 20) then Result := terbilang_int(x-10) + ' belas'&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 100) then Result := terbilang_int(x div 10) + ' puluh' + terbilang_int(x mod 10)&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 200) then Result := ' Seratus' + terbilang_int(x-100)&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 1000) then Result := terbilang_int(x div 100) + ' ratus' + terbilang_int(x mod 100)&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 2000) then Result := ' Seribu' + terbilang_int(x-1000)&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 1000000) then Result := terbilang_int(x div 1000) + ' Ribu' + terbilang_int(x mod 1000)&#13;&#10;&#13;&#10;    else&#13;&#10;&#13;&#10;    if (x &#60; 1000000000) then Result := terbilang_int(x div 1000000) + ' Juta' + terbilang_int(x mod 1000000) ;&#13;&#10;&#13;&#10;    end;      &#13;&#10;&#13;&#10;begin                                    &#13;&#10;&#13;&#10;   d := trimall(d);&#13;&#10;&#13;&#10;   if length(d) = 0 then                                                   &#13;&#10;&#13;&#10;      begin&#13;&#10;&#13;&#10;       result := '';                                             &#13;&#10;&#13;&#10;       exit;&#13;&#10;&#13;&#10;      end;                         &#13;&#10;&#13;&#10;   if length(d) &#60; 10  then result := terbilang_int(strtoint(d)) else&#13;&#10;&#13;&#10;   if length(d) &#60; 13  then&#13;&#10;&#13;&#10;       begin&#13;&#10;&#13;&#10;       divX := copy(d,1,length(d)-9);&#13;&#10;&#13;&#10;       modX := copy(d,length(d)-8,9);&#13;&#10;&#13;&#10;       if strtoint(divX) &#60;&#62; 0 then grop := ' milyar' else grop := '';       &#13;&#10;&#13;&#10;       result := terbilang(divX)+grop+terbilang(modX);&#13;&#10;&#13;&#10;       end  else&#13;&#10;&#13;&#10;   if length(d) &#60; 16 then&#13;&#10;&#13;&#10;       begin&#13;&#10;&#13;&#10;       divX := copy(d,1,length(d)-12);&#13;&#10;&#13;&#10;       modX := copy(d,length(d)-11,12);&#13;&#10;&#13;&#10;       if strtoint(divX) &#60;&#62; 0 then grop := ' trilyun' else grop := '';       &#13;&#10;&#13;&#10;       result := terbilang(divX)+grop+terbilang(modX);&#13;&#10;&#13;&#10;       end else result := '0';&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;  &#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;end.    &#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="044C65667403E80003546F7003050108446174617365747301010C3400000020446174615365743D22667278444244617461736574312220446174615365744E616D653D2266727844424461746173657431220000095661726961626C65730100055374796C650100">
