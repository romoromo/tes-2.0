<TfrxReport Version="4.5" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40616,9897182407" ReportOptions.Description.Text="" ReportOptions.LastChange="40617,8574209606" ScriptLanguage="PascalScript" ScriptText.Text="          &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;function terbilang(d:string):string;&#13;&#10;var&#13;&#10;    divX,modX,grop : string;&#13;&#10;function trimall(STR:string):string;&#13;&#10;  var&#13;&#10;  i : integer;&#13;&#10;  str2 : string;                                  &#13;&#10;begin&#13;&#10;  i := 0;&#13;&#10;  for i := 1 to length(str) do&#13;&#10;  begin&#13;&#10;  if (str[i] in ['0'..'9']) then str2 := str2 + str[i];                                                                                                            &#13;&#10;  end;&#13;&#10;  RESULT := str2;                                  &#13;&#10;end;          &#13;&#10;  function bil(x:integer):string;&#13;&#10;  begin&#13;&#10;    case x of&#13;&#10;      0 : result := '';&#13;&#10;      1 : result := 'satu';&#13;&#10;      2 : result := 'dua';              &#13;&#10;      3 : result := 'tiga';              &#13;&#10;      4 : result := 'empat';                &#13;&#10;      5 : result := 'lima';              &#13;&#10;      6 : result := 'enam';              &#13;&#10;      7 : result := 'tujuh';                &#13;&#10;      8 : result := 'delapan';                    &#13;&#10;      9 : result := 'sembilan';                      &#13;&#10;      10 : result :='sepuluh';                    &#13;&#10;      11 : result :='sebelas';                              &#13;&#10;      end;                &#13;&#10;  end;      &#13;&#10;  function terbilang_int(x:integer):string;&#13;&#10;    begin &#13;&#10;    if (x &#60; 12) then Result := ' ' + bil(x) else if (x &#60; 20) then Result := terbilang_int(x-10) + ' belas'&#13;&#10;    else&#13;&#10;    if (x &#60; 100) then Result := terbilang_int(x div 10) + ' puluh' + terbilang_int(x mod 10)&#13;&#10;    else&#13;&#10;    if (x &#60; 200) then Result := ' seratus' + terbilang_int(x-100)&#13;&#10;    else&#13;&#10;    if (x &#60; 1000) then Result := terbilang_int(x div 100) + ' ratus' + terbilang_int(x mod 100)&#13;&#10;    else&#13;&#10;    if (x &#60; 2000) then Result := ' seribu' + terbilang_int(x-1000)&#13;&#10;    else&#13;&#10;    if (x &#60; 1000000) then Result := terbilang_int(x div 1000) + ' ribu' + terbilang_int(x mod 1000)&#13;&#10;    else&#13;&#10;    if (x &#60; 1000000000) then Result := terbilang_int(x div 1000000) + ' juta' + terbilang_int(x mod 1000000) ;&#13;&#10;    end;      &#13;&#10;begin                                    &#13;&#10;   d := trimall(d);&#13;&#10;   if length(d) = 0 then                                                   &#13;&#10;      begin&#13;&#10;       result := '';                                             &#13;&#10;       exit;&#13;&#10;      end;                         &#13;&#10;   if length(d) &#60; 10  then result := terbilang_int(strtoint(d)) else&#13;&#10;   if length(d) &#60; 13  then&#13;&#10;       begin&#13;&#10;       divX := copy(d,1,length(d)-9);&#13;&#10;       modX := copy(d,length(d)-8,9);&#13;&#10;       if strtoint(divX) &#60;&#62; 0 then grop := ' milyar' else grop := '';       &#13;&#10;       result := terbilang(divX)+grop+terbilang(modX);&#13;&#10;       end  else&#13;&#10;   if length(d) &#60; 16 then&#13;&#10;       begin&#13;&#10;       divX := copy(d,1,length(d)-12);&#13;&#10;       modX := copy(d,length(d)-11,12);&#13;&#10;       if strtoint(divX) &#60;&#62; 0 then grop := ' trilyun' else grop := '';       &#13;&#10;       result := terbilang(divX)+grop+terbilang(modX);&#13;&#10;       end else result := '0';&#13;&#10;end;&#13;&#10;  &#13;&#10;procedure Memo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;                                 &#13;&#10;  memo1.text := terbilang('');                                                                    &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="044C656674026003546F7003D8010844617461736574730100095661726961626C65730100055374796C650100">
