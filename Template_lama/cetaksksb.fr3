<TfrxReport Version="4.5" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40614,3563290278" ReportOptions.Description.Text="" ReportOptions.LastChange="41242,4972031829" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  wait : boolean;                                           &#13;&#10;&#13;&#10;function trimall(STR:string):string;&#13;&#10;  var&#13;&#10;  i : integer;&#13;&#10;  str2 : string;                                  &#13;&#10;begin&#13;&#10;  i := 0;&#13;&#10;  for i := 1 to length(str) do&#13;&#10;  begin&#13;&#10;  if (str[i] in ['0'..'9']) then str2 := str2 + str[i];                                                                                                            &#13;&#10;  end;&#13;&#10;  RESULT := str2;                                  &#13;&#10;end;&#13;&#10;  &#13;&#10;function terbilang(sValue: string):string;&#13;&#10;var&#13;&#10;sPattern: string = '000000000000000';    &#13;&#10;S,kata : string;&#13;&#10;Satu, Dua, Tiga, Belas, Gabung: string;&#13;&#10;Sen, Sen1, Sen2: string;&#13;&#10;Hitung : integer;&#13;&#10;one, two, three: integer;&#13;&#10;Angka : array [0..19] of string =['', 'Satu', 'Dua', 'Tiga', 'Empat','Lima', 'Enam', 'Tujuh', 'Delapan','Sembilan', 'Sepuluh', 'Sebelas','Duabelas', 'Tigabelas', 'Empatbelas','Limabelas', 'Enambelas', 'Tujuhbelas','Delapanbelas', 'Sembilanbelas'];&#13;&#10;    &#13;&#10;&#13;&#10;begin&#13;&#10;wait := true;                              &#13;&#10;One := 4;&#13;&#10;Two := 5;&#13;&#10;Three := 6;&#13;&#10;Hitung := 1;&#13;&#10;Kata := '';&#13;&#10;S := copy(sPattern, 1, length(sPattern) - length(trim(sValue))) + sValue;&#13;&#10;Sen1 := Copy(S, 14, 1);&#13;&#10;Sen2 := Copy(S, 15, 1);&#13;&#10;Sen := Sen1 + Sen2;&#13;&#10;while Hitung &#60; 5 do&#13;&#10;begin&#13;&#10;Satu := Copy(S, One, 1);&#13;&#10;Dua := Copy(S, Two, 1);&#13;&#10;Tiga := Copy(S, Three, 1);&#13;&#10;Gabung := Satu + Dua + Tiga;&#13;&#10;&#13;&#10;if StrToInt(Satu) = 1 then&#13;&#10;Kata := Kata + 'Seratus '&#13;&#10;else&#13;&#10;if StrToInt(Satu) &#62; 1 Then&#13;&#10;Kata := Kata + Angka[StrToInt(satu)] + ' Ratus ';&#13;&#10;&#13;&#10;if StrToInt(Dua) = 1 then&#13;&#10;begin&#13;&#10;Belas := Dua + Tiga;&#13;&#10;Kata := Kata + Angka[StrToInt(Belas)];&#13;&#10;end&#13;&#10;else&#13;&#10;if StrToInt(Dua) &#62; 1 Then&#13;&#10;Kata := Kata + Angka[StrToInt(Dua)] + ' Puluh ' +&#13;&#10;Angka[StrToInt(Tiga)]&#13;&#10;else&#13;&#10;if (StrToInt(Dua) = 0) and (StrToInt(Tiga) &#62; 0) Then&#13;&#10;begin&#13;&#10;if ((Hitung = 3) and (Gabung = '001')) or&#13;&#10;((Hitung = 3) and (Gabung = ' 1')) then&#13;&#10;Kata := Kata + 'Seribu '&#13;&#10;else&#13;&#10;Kata := Kata + Angka[StrToInt(Tiga)];&#13;&#10;end;&#13;&#10;&#13;&#10;if (hitung = 1) and (StrToInt(Gabung) &#62; 0) then&#13;&#10;Kata := Kata + ' Milyar '&#13;&#10;else&#13;&#10;if (Hitung = 2) and (StrToInt(Gabung) &#62; 0) then&#13;&#10;Kata := Kata + ' Juta '&#13;&#10;else&#13;&#10;if (Hitung = 3) and (StrToInt(Gabung) &#62; 0) then&#13;&#10;begin&#13;&#10;if (Gabung = '001') or (Gabung = ' 1') then&#13;&#10;Kata := Kata + ''&#13;&#10;else&#13;&#10;Kata := Kata + ' Ribu ';&#13;&#10;end;&#13;&#10;Hitung := Hitung + 1;&#13;&#10;One := One + 3;&#13;&#10;Two := Two + 3;&#13;&#10;Three := Three + 3;&#13;&#10;end;&#13;&#10;if length(Kata) &#62; 1 then Kata := Kata + ' Rupiah ';&#13;&#10;Result := Kata;&#13;&#10;wait := false;                                &#13;&#10;end;&#13;&#10;    &#13;&#10;&#13;&#10;&#13;&#10;procedure mterbilangOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;end." StoreInDFM="False" PropData="">
